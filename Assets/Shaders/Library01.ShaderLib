    float4 UV0AND1 : TEXCOORD0;
  #endif
    float3 Normal  : TEXCOORD1;
  #ifdef USE_TANGENT
    float3 Tangent : TEXCOORD2;
    float3 BiTangent: TEXCOORD3;
  #endif
  
  #ifdef PROJECTOR_UV
    float4 UVProjAndAttenuation : TEXCOORD4;
  #endif
  
