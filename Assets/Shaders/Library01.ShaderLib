  Out.ScreenPosition = Out.ProjPos;
  Out.ScreenPosition.y = -Out.ScreenPosition.y;
  Out.ScreenPosition.z = GetLinearDepth(mul(matMV, float4(ObjPos.xyz, 1.0f)).z, contextCP.y);

