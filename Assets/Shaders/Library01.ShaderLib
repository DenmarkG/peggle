    Out.ProjDir.x = dot(Out.LightWS.xyz,RefPlaneX.xyz);
    Out.ProjDir.y = dot(Out.LightWS.xyz,RefPlaneY.xyz);
    Out.ProjDir.z = dot(Out.LightWS.xyz,RefPlaneW.xyz);
  #endif
