    #endif
		
		result.a = Opacity;
		
		return result;
	}
	
	float4 Lighting_LightGridSimple( float4 DiffuseColor, float3 Normal, float SpecularExponent, float3 SpecularColor, float SpecularMultiplier, float3 AmbientColor, float Opacity, float3 EyeNorm )
