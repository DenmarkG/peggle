     		float SpecProd = GetSpecularIlluminationPoint(EyeDir + NormLightVec, Normal, SpecularExponent);
     		SpecProd *= SpecularMultiplier;
     		SpecProd = saturate(SpecProd * DotProd);
     		ResColor += (SpecMul * SpecProd * float4(LightColor.rgb, 0.0));	
