  	Out.UVProjAndAttenuation.y = 0.5 * distq + distt;
  	Out.UVProjAndAttenuation.w = distq;
  	Out.UVProjAndAttenuation.z = dot(worldPos, RefPlaneAtt); // Attenuation is stored in the z component
  #endif
  
