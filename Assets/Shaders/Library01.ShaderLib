     		float4 SpecMul = float4(SpecularColor.rgb, 1.0);
     		SpecMul *= SpecularMultiplier;
     		cResColor.rgb += difspec.specular.rgb * SpecMul.rgb;
     	#endif
  		cResColor.a = Opacity;
  		
  		return cResColor;
